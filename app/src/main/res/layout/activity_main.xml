<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="android跳转到flutter界面"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tv_click_with_new_engine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="withNewEngine"
        app:layout_constraintEnd_toEndOf="@+id/tv_title"
        app:layout_constraintStart_toStartOf="@+id/tv_title"
        app:layout_constraintTop_toBottomOf="@+id/tv_title" />

    <Button
        android:id="@+id/tv_click_engine"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="FlutterEngine"
        app:layout_constraintEnd_toEndOf="@+id/tv_click_with_new_engine"
        app:layout_constraintStart_toStartOf="@+id/tv_click_with_new_engine"
        app:layout_constraintTop_toBottomOf="@+id/tv_click_with_new_engine" />

</androidx.constraintlayout.widget.ConstraintLayout>